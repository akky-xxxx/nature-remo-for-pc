{
  "name": "nature-remo-for-pc",
  "productName": "NatureRemoForPc",
  "version": "1.0.0",
  "author": "akky-xxxx <114.s.aki@gmail.com>",
  "main": "main/index.js",
  "scripts": {
    "build": "run-s build:renderer build:electron",
    "build:electron": "tsc -p src/electron",
    "build:renderer": "next build src/renderer && next export src/renderer",
    "check-all": "npm-run-all -s fix -p check-code",
    "check-code": "run-p spell-check lint type-check",
    "clean": "rimraf dist main src/renderer/out src/renderer/.next  src/renderer/out",
    "dev": "yarn build:electron && electron .",
    "dist": "yarn build && electron-builder",
    "fix": "run-p fix:*",
    "fix:pack": "fixpack",
    "fix:ts": "eslint './src/**/*.{ts,tsx}' --fix",
    "lint": "run-p lint:*",
    "lint:css": "stylelint './src/**/*.{ts,tsx}'",
    "lint:ts": "eslint './src/**/*.{ts,tsx}'",
    "pack-app": "yarn build && electron-builder --dir",
    "spell-check": "cspell './src/**/*.{ts,tsx}'",
    "type-check": "run-p type-check:*",
    "type-check:electron": "tsc -p ./src/electron",
    "type-check:renderer": "tsc -p ./src/renderer"
  },
  "dependencies": {
    "electron-is-dev": "^1.1.0",
    "electron-next": "^3.1.5",
    "react": "^17.0.0",
    "react-dom": "^17.0.0"
  },
  "devDependencies": {
    "@types/node": "^14.14.6",
    "@types/react": "^16.9.9",
    "@types/react-dom": "^16.9.9",
    "@typescript-eslint/eslint-plugin": "^4.14.1",
    "@typescript-eslint/parser": "^4.14.1",
    "cspell": "^5.2.4",
    "electron": "^10.1.5",
    "electron-builder": "^22.9.1",
    "eslint": "^7.18.0",
    "eslint-config-airbnb": "^18.2.1",
    "eslint-config-airbnb-base": "^14.2.1",
    "eslint-config-prettier": "^7.2.0",
    "eslint-import-resolver-typescript": "^2.3.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-prettier": "^3.3.1",
    "eslint-plugin-react": "^7.22.0",
    "eslint-plugin-react-hooks": "^4.2.0",
    "fixpack": "^4.0.0",
    "next": "latest",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.2.1",
    "rimraf": "^3.0.0",
    "stylelint": "^13.9.0",
    "stylelint-config-prettier": "^8.0.2",
    "stylelint-config-standard": "^20.0.0",
    "stylelint-config-styled-components": "^0.1.1",
    "stylelint-order": "^4.1.0",
    "stylelint-prettier": "^1.1.2",
    "stylelint-processor-styled-components": "^1.10.0",
    "typescript": "^4.0.5"
  },
  "build": {
    "asar": true,
    "files": [
      "main",
      "renderer/out"
    ]
  },
  "license": "MIT"
}
