{
  "name": "nature-remo-for-pc",
  "productName": "NatureRemoForPc",
  "version": "1.1.1",
  "author": "akky-xxxx <114.s.aki@gmail.com>",
  "main": "main/index.js",
  "scripts": {
    "api-mock": "node spec/api-mock/index.js",
    "build": "run-p build:*",
    "build:electron": "tsc -p ./src/electron",
    "build:renderer": "next build ./src/renderer && next export ./src/renderer",
    "check-all": "npm-run-all -s fix -p check-code",
    "check-code": "run-p spell-check lint type-check",
    "clean": "rimraf dist ./main ./src/renderer/out ./src/renderer/.next  ./src/renderer/out",
    "dev": "npm-run-all -s build:electron -p run:electron api-mock",
    "dist": "yarn build && electron-builder",
    "fix": "run-p fix:*",
    "fix:pack": "fixpack",
    "fix:ts": "eslint './**/*.{ts,tsx}' --fix",
    "lint": "run-p lint:*",
    "lint:css": "stylelint './**/*.{ts,tsx}'",
    "lint:ts": "eslint './**/*.{ts,tsx}'",
    "pack-app": "yarn build && electron-builder --dir",
    "run:electron": "electron .",
    "spell-check": "cspell './**/*.{ts,tsx}'",
    "type-check": "run-p type-check:*",
    "type-check:electron": "tsc -p ./src/electron",
    "type-check:renderer": "tsc -p ./src/renderer"
  },
  "dependencies": {
    "@ant-design/icons": "^4.5.0",
    "antd": "^4.12.3",
    "array-move": "^3.0.1",
    "axios": "^0.21.1",
    "dotenv": "^8.2.0",
    "electron-is-dev": "^1.1.0",
    "electron-log": "^4.3.2",
    "electron-next": "^3.1.5",
    "express": "^4.17.1",
    "form-data": "^4.0.0",
    "next": "10.0.7",
    "react": "^17.0.0",
    "react-dom": "^17.0.0",
    "react-sortable-hoc": "^1.11.0"
  },
  "devDependencies": {
    "@types/node": "^14.14.31",
    "@types/react": "^17.0.2",
    "@types/react-dom": "^17.0.1",
    "@typescript-eslint/eslint-plugin": "^4.15.2",
    "@typescript-eslint/parser": "^4.15.2",
    "cspell": "^5.2.4",
    "electron": "^11.3.0",
    "electron-builder": "^22.9.1",
    "eslint": "^7.20.0",
    "eslint-config-airbnb": "^18.2.1",
    "eslint-config-airbnb-base": "^14.2.1",
    "eslint-config-prettier": "^8.0.0",
    "eslint-import-resolver-typescript": "^2.4.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-prettier": "^3.3.1",
    "eslint-plugin-react": "^7.22.0",
    "eslint-plugin-react-hooks": "^4.2.0",
    "fixpack": "^4.0.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.2.1",
    "rimraf": "^3.0.0",
    "stylelint": "^13.11.0",
    "stylelint-config-prettier": "^8.0.2",
    "stylelint-config-standard": "^20.0.0",
    "stylelint-config-styled-components": "^0.1.1",
    "stylelint-order": "^4.1.0",
    "stylelint-prettier": "^1.1.2",
    "stylelint-processor-styled-components": "^1.10.0",
    "typescript": "^4.1.5"
  },
  "build": {
    "asar": true,
    "files": [
      "./main",
      "./src/renderer/out",
      ".env"
    ]
  },
  "license": "MIT"
}
